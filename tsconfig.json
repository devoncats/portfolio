{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/layouts": [
        "src/layouts/*"
      ],
      "@/icons": [
        "src/icons/*"
      ],
    },
  },
  "extends": "astro/tsconfigs/strict",
}