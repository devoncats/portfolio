{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/layouts": [
        "src/layouts/*"
      ],
      "@/icons": [
        "src/icons/*"
      ],
      "@/generic": [
        "src/components/generic/*"
      ]
    },
    "jsxImportSource": "react"
  },
  "extends": "astro/tsconfigs/strict"
}